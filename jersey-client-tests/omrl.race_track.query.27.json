{
  "select": [
    [ "name" ],
    [ "cost", "cost" ]
  ],
  "from": "race",
  "where": [ "AND", [ "=", [ "cost", "year" ], "2020" ],
                    [ "=", [ "cost", "quarter" ], "Q1" ],
                    [ ">", [ "cost", "cost" ], 10 ],
                    [ "=", [ "name" ], "whatever" ]
           ]
}