{
  "comment": "column_expand from a link track -> races ...",

  "select": [
    [ "name" ],
    [ "location" ],
    [ "races", "cost" ],
    [ "races", "year" ],
    [ "races", "period" ]
  ],
  "from": "track",
  "where": [ "AND", [ "=", [ "races", "year" ], "2020" ],
                    [ "=", [ "races", "quarter" ], "Q1" ],
                    [ "=", [ "races", "period" ], "ITD" ],
                    [ "=", [ "races", "type" ], "LBR" ] ],
  "limit": 50
}