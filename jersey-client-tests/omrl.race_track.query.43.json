{
  "select": [
    [ "name" ],
    [ "cost", "cost" ],
    [ "cost", "year" ],
    [ "cost", "quarter" ]
  ],
  "from": "race",
  "where": [ "OR", [ "AND", [ "=", [ "cost", "year" ], "2020" ],
                            [ "=", [ "cost", "quarter" ], "Q1" ],
                            [ "=", [ "cost", "period" ], "ITD" ],
                            [ "=", [ "cost", "type" ], "LBR" ]
                   ],
                   [ "=", [ "name" ], "RaceTrack whatever" ]
           ],
  "order_by": [
    [ "#DESC", [ "cost", "cost" ]]
  ]
}