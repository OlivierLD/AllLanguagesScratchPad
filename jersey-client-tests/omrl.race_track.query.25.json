{
  "comment": "race_stuff is a column expansion",
  "select": [
    [ "name" ],
    [ "race_stuff", "race_stuff" ],
    [ "race_stuff", "stuff" ]
  ],
  "from": "race",
  "where": [ "AND", [ "=", [ "race_stuff", "stuff" ], "DATE" ],
                    [ "=", [ "name" ], "Lime Rock GT Classic 250" ]
           ]
}